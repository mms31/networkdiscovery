<p id="notice"><%= notice %></p>

<h1>Listing Scanhosts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scanhosts.each do |scanhost| %>
      <tr>
        <td><%= link_to 'Show', scanhost %></td>
        <td><%= link_to 'Edit', edit_scanhost_path(scanhost) %></td>
        <td><%= link_to 'Destroy', scanhost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scanhost', new_scanhost_path %>
